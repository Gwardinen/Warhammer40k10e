    "assetCatalog": {
      "Ability§Aggressive Assault": {
        "text": "Each time this model makes a ranged attack against the closest eligible target, add 1 to the Hit roll.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Chainbreaker": {
        "text": "Each time this model makes an attack, an\nunmodifed successful Hit roll of 5+ scores a Critical Hit.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Code Chivalric": {
        "keywords": {
          "Keywords": [
            "Faction"
          ]
        }
      },
      "Ability§Crusader’s Duty (bondsman)": {
        "text": "While a model is affected by this ability, each time that model makes a ranged attack, add 1 to the Hit roll.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Errant’s Duty (bondsman)": {
        "text": "While a model is affected by this ability, you can re-roll Advance rolls made for that model and its ranged weapons have the [ASSAULT] ability.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Exemplar of The Code": {
        "text": "At the start of the battle, select one unit from your opponent’s army. Each time this model makes an attack that targets that unit, you can re-roll the Hit roll.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Gallant’s Duty (bondsman)": {
        "text": "While a model is affected by this ability, you can re-roll Charge rolls made for that model and each time that model makes a melee attack, you can re-roll the Hit roll.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Impetuous Glory": {
        "text": "Each time this model makes a Charge move, until the end of the turn, melee weapons equipped by this model have the [SUSTAINED HITS 1] ability.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Ion Aegis (aura)": {
        "text": "While a friendly Armiger model is within 6″ of this model, that Armiger model has the Benefit of Cover.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Legendary Freeblade": {
        "text": "Once per turn, you can target this\nmodel with a Stratagem for 0CP, and can do so even if you\nhave already targeted a different unit with that Stratagem in\nthe same phase.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Martial Pride": {
        "text": "Each time a melee attack targets this model, subtract 1 from the Hit roll.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Mentor (bondsman)": {
        "text": "While a model is affected by this ability, improve that model’s Leadership characteristic by 1 and improve that model’s Objective Control characteristic by 2.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Overwhelming Firestorm": {
        "text": "In your Shooting phase, after this model has shot, select one enemy unit hit by this model this phase. That unit must take a Battle-shock test.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Paladin’s Duty (bondsman)": {
        "text": "While a model is affected by this ability, its weapons have the [LETHAL HITS] and [LANCE] abilities.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Punishing Salvoes": {
        "text": "In your Movement phase, if this model Remains Stationary, until the start of your next Movement phase, this model’s ranged weapons have the [SUSTAINED HITS 1] ability.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Rapid Fire D6+3": {},
      "Ability§Seasoned Noble": {
        "text": "Once per phase, you can re-roll one Hit roll, one Wound roll or one saving throw made for this model.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Skyfire Protocols": {
        "text": "While this model is either wholly within your deployment zone or within range of an objective marker you control, its Armiger autocannons have the [ANTI-FLY 2+] ability.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Super-heavy Walker": {
        "keywords": {
          "Keywords": [
            "Faction"
          ]
        }
      },
      "Ability§Thin Their Ranks": {
        "text": "Each time this model makes a ranged attack that targets an enemy unit (excluding Monsters and Vehicles), that attack has the [DEVASTATING WOUNDS] ability.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Titan Hunter": {
        "text": "Each time a ranged attack made by this model is allocated to a Monster or Vehicle model, re-roll a Damage roll of 1.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
      "Ability§Using Sir Hekhtur": {
        "text": "When your Canis Rex model is destroyed, Sir Hekhtur is treated as a model disembarking from a destroyed Transport – set him up within 3″ of your Canis Rex model before it is removed. Sir Hekhtur then uses the profile, wargear, abilities and keywords shown on this side of the card, but cannot be selected as the target of any of your Stratagems other than Core Stratagems. Your Canis Rex unit is not considered to be destroyed until Sir Hekhtur is also destroyed."
      },
      "Ability§Warden’s Duty (bondsman)": {
        "text": "While a model is affected by this ability, each time an attack is allocated to that model, subtract 1 from the Damage characteristic of that attack.",
        "keywords": {
          "Keywords": [
            "Other"
          ]
        }
      },
